{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        
        "php": "^8.1",
        "symfony/symfony": "5.4.*",
        "symfony/maker-bundle": "^1.0",
        "doctrine/doctrine-bundle": "^2.13",
        "symfony/monolog-bundle": "^3.10",
        "doctrine/orm": "^2.10",
        "doctrine/dbal": "^3.0",
        "twig/twig": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "monolog/monolog": "^2.0",
        "symfony/routing": "^5.4",
        "symfony/dotenv": "*",
        "doctrine/annotations": "^2.0",
        "symfony/twig-bundle": "*",
        "symfony/asset": "*",
        "symfony/security-bundle": "^5.4",
        "symfony/security-http": "^5.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4",
        "phpunit/phpunit": "^9.5",
        "symfony/flex": "^2.8"
    },
    "scripts": {
        
        "post-install-cmd": [
            "Symfony\\Flex\\Flex::install"
        ],
        "post-update-cmd": [
            "Symfony\\Flex\\Flex::install"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
